{
  "name": "@kwc-elements/calendar",
  "version": "1.0.0",
  "description": "Beta: Calendar Web Component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/kwc-calendar/kwc-calendar.esm.js",
  "homepage": "http://kwc-elements.site/calendar",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvlvgg/kwc-elements"
  },
  "keywords": [
    "kwc-elements",
    "calendar",
    "web-components",
    "ui-component",
    "stenciljs",
    "custom-elements"
  ],
  "author": {
    "name": "kvlv.gg",
    "email": "kvlv.gg@gmail.com",
    "url": "https://github.com/kvlvgg"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.mjs"
    },
    "./hydrate": {
      "types": "./hydrate/index.d.ts",
      "import": "./hydrate/index.mjs",
      "require": "./hydrate/index.js",
      "default": "./hydrate/index.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.js",
      "default": "./loader/index.js"
    },
    "./components/*.js": {
      "types": "./dist/types/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js"
    },
    "./constants": {
      "import": "./dist/collection/constants.js",
      "types": "./dist/types/constants.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "constants": [
        "./dist/types/constants.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "dev": "stencil build --watch --output-targets=dist-custom-elements",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "@kwc-elements/utils": "workspace:*"
  }
}
